<?xml version="1.0" encoding="utf-8"?><!-- XML implementation of Card Layout -->
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_gravity="center"
    app:cardBackgroundColor="@color/light_brown"
    android:layout_margin="5dp"
    app:cardCornerRadius="15dp"
    android:elevation="5dp">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_margin="1dp"
        android:baselineAligned="false">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="25dp"
            android:layout_marginEnd="25dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tv_accountNr"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textAlignment="center"
                android:textAllCaps="true"
                android:textSize="13sp"
                android:textStyle="bold" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="40dp"
            android:layout_marginEnd="40dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tv_nrOfOrdersMade"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="1dp"
                android:textAlignment="textStart"
                android:layout_weight="1.5"
                android:textSize="11sp" />

            <TextView
                android:id="@+id/tv_nrOfGoodOrdersMade"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="1dp"
                android:textAlignment="textEnd"
                android:layout_weight="0.15"
                android:textAllCaps="true"
                android:textSize="11sp" />

            <TextView
                android:id="@+id/tv_percentOfGoodOrders"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="1dp"
                android:textAlignment="textEnd"
                android:layout_weight="1"
                android:textAllCaps="true"
                android:textSize="11sp" />


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="40dp"
            android:layout_marginEnd="40dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tv_nrOfLongOrdersMade"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="1dp"
                android:layout_weight="1.5"
                android:textAlignment="textStart"
                android:textSize="11sp" />

            <TextView
                android:id="@+id/tv_nrOfGoodLongOrdersMade"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="1dp"
                android:textAlignment="textEnd"
                android:layout_weight="0.15"
                android:textAllCaps="true"
                android:textSize="11sp"/>

            <TextView
                android:id="@+id/tv_percentOfGoodLongOrders"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="1dp"
                android:textAlignment="textEnd"
                android:layout_weight="1"
                android:textAllCaps="true"
                android:textSize="11sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="40dp"
            android:layout_marginEnd="40dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tv_nrOfShortOrdersMade"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="1dp"
                android:layout_weight="1.5"
                android:textAlignment="textStart"
                android:textSize="11sp" />

            <TextView
                android:id="@+id/tv_nrOfGoodShortOrdersMade"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="1dp"
                android:textAlignment="textEnd"
                android:layout_weight="0.15"
                android:textAllCaps="true"
                android:textSize="11sp"/>

            <TextView
                android:id="@+id/tv_percentOfGoodShortOrders"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="1dp"
                android:textAlignment="textEnd"
                android:layout_weight="1"
                android:textAllCaps="true"
                android:textSize="11sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tv_results3dPercents"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="1dp"
                android:textAlignment="textStart"
                android:textAllCaps="true"
                android:layout_weight="1"
                android:textSize="11sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/tv_results7dPercents"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="1dp"
                android:textAlignment="textEnd"
                android:textAllCaps="true"
                android:layout_weight="1"
                android:textSize="11sp"
                android:textStyle="bold" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tv_results30dPercents"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="1dp"
                android:textAlignment="textStart"
                android:textAllCaps="true"
                android:layout_weight="1"
                android:textSize="11sp"
                android:textStyle="bold" />


            <TextView
                android:id="@+id/tv_resultsAllPercents"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="1dp"
                android:textAlignment="textEnd"
                android:textAllCaps="true"
                android:layout_weight="1"
                android:textSize="11sp"
                android:textStyle="bold" />
        </LinearLayout>

    </LinearLayout>
</androidx.cardview.widget.CardView>